package com.vti.entity;

import java.time.LocalDate;
import java.util.Date;

public class Exam {
	public int id;
	public String code;
	public String title;
	public CategoryQuestion category;
	public int duration;
	public Account creator;
	public Date CreateDate;
	public Question[] questions;


	
	public Exam() {
		super();
	}



	public Exam(int id, String code, String title, int duration, Date date) {
		super();
		this.id = id;
		this.code = code;
		this.title = title;

		this.duration = duration;

		CreateDate = date;

	}

}
