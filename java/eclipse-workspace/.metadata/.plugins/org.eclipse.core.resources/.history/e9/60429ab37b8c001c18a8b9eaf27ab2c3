package com.vti.backend;

import com.vti.entity.Student;

public class Exercise3 {
	public static void question1() {
		System.out.println("------------Demo Generic----------");
		Student student1 = new Student(1, "Lap");
		Student student2 = new Student(2, "Tham");
		Student student3 = new Student(3, "Dung");
		Student student4 = new Student(4, "Hieu");
		Student student5 = new Student(5, "Quang");

		System.out.println("Thong tin hoc vien la: ");
		System.out.println(student1.toString());
		System.out.println(student2.toString());
		System.out.println(student3.toString());
		System.out.println(student4.toString());
		System.out.println(student5.toString());

	}
}
